.cell {
  display: flex;
  align-items: center;
  width: 100%;
  height: 96rpx;
  background-color: #fbf2e1;
}

.logo {
  font-size: 28rpx;
  display: flex;
  color: #bbb;
}

.logo-image {
  margin-left: 25rpx;
  width: 35rpx;
  height: 35rpx;
}

.logo-button {
  margin-top: 15rpx;
  margin-left: 340rpx;
}

.share {
  height: 56rpx;
  width: 104rpx;
  color: #69a5ed;
  border: 1px solid #69a5ed;
}

button[size=mini] {
  line-height: 2;
  font-size: 24rpx;
  padding: 0 1em;
}
